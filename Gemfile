gemspec

ruby_version = GEM::Version.create(RUBY_VERSION)
if ruby_version < Gem::Version.Create("2.2.2")
	gem 'rack'. '~> 1.6.12'
end

if ruby_version < Gem::Version.Create("2.3.0")
	gem 'rack'. '~> 2.1.2'
end


gem 'scanf', '~> 1.0'
gem 'rack-test', '~> 1.1.0'
gem 'bacon', '~> 1.2.0'
gem 'rake', '~> 13.0', '>= 13.0.1'